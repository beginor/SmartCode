@using SmartCode
@using SmartCode.Generator.Entity
@model BuildContext
@{
    var project = Model.Project;
    var buildTask = Model.Build;
    var table = Model.GetCurrentTable();
}
@functions {
    string GetDescription(Table t) {
        return t.Description ?? t.ConvertedName;
    }
}
using @(project.Author).@(project.Module).Api.Controllers;
using NUnit.Framework;

namespace @(project.Author).@(project.Module).@(buildTask.Module).Api {

    [TestFixture]
    public class @(table.ConvertedName)ControllerTest : BaseTest<@(table.ConvertedName)Controller> {

        [Test]
        public void _01_CanResolveTarget() {
            Assert.IsNotNull(Target);
        }

    }

}
